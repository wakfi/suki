{
  "name": "suki",
  "version": "0.2.1",
  "description": "Custom Discord bot",
  "main": "src/suki.js",
  "scripts": {
    "db:migrate": "yarn prisma migrate dev",
    "generate": "yarn prisma generate",
    "prestart": "tsc",
    "prepare": "yarn generate",
    "start": "node build/src/index.js",
    "fullstart": "yarn clean && yarn prestart && yarn start",
    "dev": "nodemon -w src -w tsconfig.json -w package.json -w types.d.ts -e ts,js,json,env --exec \\\"yarn\\\" -i src/languages/en-US.json -- fullstart",
    "clean": "test -d ./build && rm -r ./build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wakfi/suki.git"
  },
  "author": "wakfi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wakfi/suki/issues"
  },
  "homepage": "https://github.com/wakfi/suki#readme",
  "dependencies": {
    "@prisma/client": "4.2.1",
    "cuid": "^2.1.8",
    "discord.js": "^13.8.0",
    "dotenv": "^16.0.1",
    "moment": "^2.29.4",
    "node-fetch": "^2.6.7",
    "prisma": "^4.2.1",
    "shelljs": "^0.8.5",
    "sqlite3": "^5.0.11",
    "winston": "^3.8.1"
  },
  "packageManager": "yarn@3.2.2",
  "devDependencies": {
    "@types/node": "^18.7.7",
    "nodemon": "^2.0.19",
    "typescript": "^4.7.4"
  }
}
